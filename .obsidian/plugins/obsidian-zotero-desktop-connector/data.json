{
  "database": "Zotero",
  "noteImportFolder": "Literature Notes/",
  "pdfExportImageDPI": 120,
  "pdfExportImageFormat": "jpg",
  "pdfExportImageQuality": 90,
  "citeFormats": [
    {
      "name": "Format #1",
      "format": "formatted-bibliography",
      "cslStyle": "IEEE"
    }
  ],
  "exportFormats": [
    {
      "name": "freddie-import literature",
      "outputPathTemplate": "LiteratureNotes/{{citekey}} - {{title}}.md",
      "imageOutputPathTemplate": "LiteratureNotes/attachments/",
      "imageBaseNameTemplate": "image",
      "cslStyle": "ieee",
      "templatePath": "Templates/zotero_template.md"
    }
  ],
  "citeSuggestTemplate": "[[{{citekey}}]]",
  "openNoteAfterImport": true,
  "whichNotesToOpenAfterImport": "all-imported-notes",
  "shouldConcat": true
}